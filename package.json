  "dependencies": {
    "@sendgrid/mail": "^7.2.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jest": "^26.2.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.2",
    "moment": "^2.27.0",
